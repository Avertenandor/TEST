{
  "name": "genesis-tests",
  "version": "2.0.0",
  "description": "Complete E2E Testing Suite for GENESIS DeFi Platform",
  "private": true,
  "scripts": {
    "test": "node run_all_tests.js",
    "test:chrome": "node e2e/test_complete_chrome.js",
    "test:console": "node e2e/test_console_monitor.js",
    "test:quick": "npm run test:chrome -- --timeout=30000",
    "test:headed": "HEADLESS=false npm run test",
    "test:prod": "TEST_URL=https://crypto-processing.net/ npm run test",
    "test:local": "TEST_URL=http://localhost:5500/ npm run test",
    "test:howitworks": "node browser/test_console_how_it_works.mjs",
    "test:routes": "node browser/test_console_routes.mjs",
    "test:python": "python тесты/e2e/test_landing_chrome.py --url https://crypto-processing.net/",
    "test:vscode": "node run_all_tests.js",
    "install:python": "pip install playwright pytest",
    "clean": "rm -rf reports/* screenshots/*",
    "report": "open reports/",
    "lint": "eslint ."
  },
  "devDependencies": {
    "puppeteer": "^22.8.0",
    "playwright": "^1.40.0",
    "eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "GENESIS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Avertenandor/TEST"
  },
  "keywords": [
    "e2e",
    "testing",
    "puppeteer",
    "chrome",
    "automation",
    "genesis",
    "defi"
  ]
}
